void fib(x int) (int) (
     if (x == 0) (
          return 0;
     );
     if (x == 1) (
          return 1;
     );
     return fib(x - 1) + fib(x - 2);
);

void factor(x int) (int) (
     if (x == 0) (
          return 1;
     );
     return x * factor(x - 1);
);

meow(fib(12)); ## the answer is 144
meow(factor(5)); ## the answer is 120