const pi = 3.14;

void abs(x int) (int) (
    if (x > 0) (
        return x;
    );
    return x * -1;
);

void sqrt(x int) (int) (
    x = abs(x);
    if (x == 0) (
        return x;
    );
    if (x == 1) (
        return x;
    );
    var i = 1;
    var result = 1;
    for (result < x) (
        i = i + 1;
        result = i * i;
    );
    return i;
);